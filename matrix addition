#include<iostream>
using namespace std;
int main(){
int n1,n2;
cin>>n1>>n2;
int m1[n1][n2];
int m2[n1][n2];
for(int i=0;i<n1;i++){
for(int j=0;j<n2;j++){
cin>>m1[i][j];}
}
for(int i=0;i<n1;i++){
for(int j=0;j<n2;j++){
cin>>m2[i][j];}
}
int ans[n1][n2];
for(int i=0;i<n1;i++){
for(int j=0;j<n2;j++){
ans[i][j]=0;}
}
cout<<"The matrix one is "<<endl;
cout<<endl;
for(int i=0;i<n1;i++){
for(int j=0;j<n2;j++){
cout<<m1[i][j];}}
cout<<"The matrix two is "<<endl;
cout<<endl;
for(int i=0;i<n1;i++){
for(int j=0;j<n2;j++){
cout<<m2[i][j];}}
cout<<"The answer of addition is :"<<endl;
cout<<endl;
for(int i=0;i<n1;i++){
for(int j=0;j<n2;j++){

ans[i][j]=m1[i][j]+m2[i][j];
}
}
for(int i=0;i<n1;i++){
for(int j=0;j<n2;j++){
cout<<ans[i][j];}
}
cout<<endl;
}
